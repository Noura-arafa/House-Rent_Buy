<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>Controller.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserAdsServlet</servlet-name>
        <servlet-class>Controller.UserAdsServlet</servlet-class>
    </servlet>
    <servlet>

        <servlet-name>HouseImage</servlet-name>
        <servlet-class>Controller.HouseImage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editServlet</servlet-name>
        <servlet-class>Controller.editServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editHouseinDBServlet</servlet-name>
        <servlet-class>Controller.editHouseinDBServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>specificHouseServlet</servlet-name>
        <servlet-class>Controller.specificHouseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteHouseServlet</servlet-name>
        <servlet-class>Controller.deleteHouseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchServlet</servlet-name>
        <servlet-class>Controller.searchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>viewPhotoServlet</servlet-name>
        <servlet-class>Controller.viewPhotoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>rateServlet</servlet-name>
        <servlet-class>Controller.rateServlet</servlet-class>
    </servlet>
   <servlet-mapping>
        <servlet-name>searchServlet</servlet-name>
        <url-pattern>/searchServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserAdsServlet</servlet-name>
        <url-pattern>/UserAdsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>

        <servlet-name>HouseImage</servlet-name>
        <url-pattern>/HouseImage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editServlet</servlet-name>
        <url-pattern>/editServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editHouseinDBServlet</servlet-name>
        <url-pattern>/editHouseinDBServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>specificHouseServlet</servlet-name>
        <url-pattern>/specificHouseServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteHouseServlet</servlet-name>
        <url-pattern>/deleteHouseServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>viewPhotoServlet</servlet-name>
        <url-pattern>/viewPhotoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>rateServlet</servlet-name>
        <url-pattern>/rateServlet</url-pattern>

    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
